program Kabisat;
uses crt;
var
   tahun: integer;

begin
    clrscr;
    writeln('PILIH TAHUN: ');
    readln(tahun);

    if (tahun mod 4 = 0) and (tahun mod 100 <> 0) or (tahun mod 400 = 0) then
      writeln(tahun, ' Itu Tahun Kabisat')
    else
      writeln(tahun, ' Bukan Tahun Kabisat:');
end.
